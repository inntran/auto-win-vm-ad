VM_RAM=2048
VM_CPUS=1

VM_DISKSIZE=32
DATA_DISKSIZE=10

IFACE_COUNT=1

ADMINPASSWORD=$ADDS_ADMIN_PASSWORD
SAFEADMINPASSWORD=$ADDS_SAFEADMIN_PASSWORD

FIRSTBOOT_SCRIPT="newdomain.cmd"
PRIMARY_DNS=$VM_IP

if grep -q MASTER_DC_IP $BASEDIR/domain.info; then
  grep MASTER_DC_IP $BASEDIR/domain.info
  sed -i '/MASTER_DC_IP/d' $BASEDIR/domain.info
fi

echo "MASTER_DC_IP=$VM_IP" >> $BASEDIR/domain.info
